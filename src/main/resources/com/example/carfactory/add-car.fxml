<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.carfactory.AddCar">
   <ChoiceBox fx:id="fuelChoiceBox" layoutX="194.0" layoutY="101.0" prefWidth="150.0">
      <String fx:value="Benzyna" />
      <String fx:value="Diesel" />
      <String fx:value="Hybryda" />
      <String fx:value="Elektryczny" />
   </ChoiceBox>
   <TextField fx:id="engineCapacityTextBox" layoutX="14.0" layoutY="101.0" promptText="Pojemność silnika" />
   <TextField fx:id="brandTextBox" layoutX="14.0" layoutY="32.0" promptText="Marka" />
   <TextField fx:id="modelTextBox" layoutX="194.0" layoutY="32.0" promptText="Model" />
   <Button fx:id="submitButton" layoutX="274.0" layoutY="252.0" mnemonicParsing="false" onAction="#onSubmitButtonClick" text="Zatwierdź" />
   <ChoiceBox fx:id="bodyChoiceBox" layoutX="14.0" layoutY="175.0" prefWidth="150.0">
      <String fx:value="SUV" />
      <String fx:value="Hatchback" />
      <String fx:value="Sedan" />
      <String fx:value="Coupe" />
   </ChoiceBox>
   <ChoiceBox fx:id="interiorChoiceBox" layoutX="199.0" layoutY="175.0" prefWidth="150.0">
      <String fx:value="Basic" />
      <String fx:value="Comfort" />
      <String fx:value="Premium" />
   </ChoiceBox>
</AnchorPane>
